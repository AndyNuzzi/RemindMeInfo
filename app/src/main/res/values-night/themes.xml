<resources xmlns:tools="http://schemas.android.com/tools">
    <!--Baseapplicationtheme.-->
    <style name="Theme.RemindMeInfo" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!--Primarybrandcolor.-->
        <item name="colorPrimary">@color/blue</item>
        <item name="colorPrimaryVariant">@color/purple</item>
        <item name="colorOnPrimary">@color/gray</item>
        <item name="colorOnBackground">@color/dark_gray</item>
        <item name="colorButtonNormal">@color/light_gray</item>
        <!--Secondarybrandcolor.-->
        <item name="colorSecondary">@color/dark_orange</item>
        <item name="colorSecondaryVariant">@color/light_gray</item>
        <item name="colorOnSecondary">@color/gray</item>
        <item name="color">@color/pink</item>
        <!--Statusbarcolor.-->
        <item name="android:statusBarColor">@color/purple</item>
        <!--Customizeyourthemehere.-->
    </style>

    <style name="Theme.RemindMeInfo.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.RemindMeInfo.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.RemindMeInfo.PopupOverlay" parent="ThemeOverlay.AppCompat.Dark" />

</resources>